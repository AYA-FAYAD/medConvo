{
  "name": "med-convo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/med-convo/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/node:build",
      "options": {
        "outputPath": "dist/apps/med-convo",
        "main": "apps/med-convo/src/main.ts",
        "tsConfig": "apps/med-convo/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "med-convo:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "med-convo:build:development"
        },
        "production": {
          "buildTarget": "med-convo:build:production"
        }
      }
    }
  },
  "tags": []
}
